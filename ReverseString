class Solution {
    public void reverseString(char[] s) {
        Stack<Character> stack = new Stack<Character>();
        int i=0;
        for (i=0; i<s.length; i++){
                stack.push(s[i]);
        }
        for (i=0; i<s.length; i++){
                s[i] = stack.pop();
        }
        System.out.println(s);
    }
}
